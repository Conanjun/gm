#pz_dentales input[type=checkbox] ,
#pz_dentales_2 input[type=checkbox] {
	display: none;
}

#img_pz,
#img_pz_2 {
	position: relative;
}

#pz_dentales,
#pz_dentales_2,
#pz_dentales_3,
#pz_dentales_5,
#pz_dentales_7 {
	width: 850px;
	height: 456px;
	background: url('../images/pz/image_radiografia.jpg') no-repeat;
}

input[type=checkbox] + label ,
input[type=checkbox]:checked + label {
	background-repeat: no-repeat;
	background-size: contain;
	display: inline-block;
	padding: 0 0 0 0px;
}

