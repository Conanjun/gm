    <script>
    
        $(function(){

            function demo () {
                alert('s');
            }

            // 展开消息
            $('#message_trigger_ok').on('click', function (e) {
                e.preventDefault();
                $.scojs_message('This is an info message', $.scojs_message.TYPE_OK);
            });
            $('#message_trigger_err').on('click', function (e) {
                e.preventDefault();
                $.scojs_message('This is an error message', $.scojs_message.TYPE_ERROR);
            });

            // table增加
            $('.table').on('click', '.btn-del', function(){
                $(this).closest('tr').slideUp(function(){
                    $(this).remove();
                })
            })

            $('#tr-add').on('click', function(){
               var _table = $(this).parent().prev('.table').find('tbody');
               _table.append(_table.find('tr').eq(0).clone());
            })

            $('.ibox-form .fa-plus').on('click', function(){
                $(this).closest('.form-group').before( $(this).closest('.form-group').prev().clone(true) );
            })

            $('.ibox-form').on('click', '.fa-times', function(){
                $(this).closest('.form-group').slideUp(function(){
                    $(this).remove();
                })
            })

            // 
            // $.menu();        菜单定位
            // $.mask.open();   打开mask
            // $.mask.close();  关闭mask
            // 

            // 打开菜单
            $.menu();

            // 打开mask
            $('#maskIn').on('click', function(){
                $.mask.open();
                
                setTimeout(function(){
                    $.mask.close();
                },1500)
            })

        })

    </script>

