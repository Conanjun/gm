    <script>
    
        $(function(){

            $('.ui-select').chosen();
            $('.i-checks').iCheck({
                checkboxClass: 'icheckbox_square-green',
                radioClass: 'iradio_square-green',
            });

            // 
            $('tr.active').on('click', function(){
                var _this = $(this),
                    _info = '';
                if ( !_this.hasClass('up') && _this.attr('data-url') && _this.next('.infos').length == 0 ) {
                       console.log('s');
                       _this.addClass('up');
                       _info += '<tr class="infos">' +
                                 '  <td>info1</td>'+
                                 '  <td>info2</td>'+
                                 '  <td>info3</td>'+
                                 '  <td>info4</td>'+
                                 '  <td>info5</td>'+
                                 '  <td>info6</td>'+
                                 '</tr>'
                        +'';
                        _this.after(_info);
                } else if ( _this.attr('data-url') && _this.next('.infos').length >= 1 && !_this.hasClass('up') ) {
                    _this.addClass('up');
                    _this.next('.infos').show();
                } else if ( _this.hasClass('up') ) {
                    _this.removeClass('up');
                    _this.next('.infos').hide();
                }
            })

        })

    </script>

