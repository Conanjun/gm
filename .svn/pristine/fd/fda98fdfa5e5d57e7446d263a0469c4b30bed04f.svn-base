                <tr>
                    <th></th>
                    <td>
                       <div class="rightdiv">
                        <a href="javascript:history.go(-1);" class="btn btn-back"><i class="icon-goback icon-level-up icon-large icon-rotate-270"></i> 返回</a>
                     <a href="javascript:;" id="submit" class="btn btn-primary" onclick="$('#l1').show();$('#l2').show();$('form').submit();"
                           data-loading="稍候..."><i class="icon-save"></i> 提交</a></div></td>
                    <td></td>
                </tr>
